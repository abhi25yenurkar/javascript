   var str1="virat";
   var str2="Anushka";
   var temp;
   function swap(value1,value2) {
    console.log("before swap:-",value1,value2);
    temp=value1;
    value1=value2;
    value2=temp;
    console.log("After swap:-",value1,value2);
   }
     swap(str1,str2);
    


     var num1=1000, num2=2000;
      swap (num1,num2);


