      console.log("******** STEP4*********");

      function addThreeValues(num1,num2,num3) {
        var sum=num1+num2+num3;
        return sum;
      }
      var sumResult=addThreeValues(10.23,600,40);
      console.log(sumResult);

      var value1="hello  ",value2="Good   ",value3="Morning   ";
      var Result=addThreeValues(value1,value2,value3);
      console.log(Result);